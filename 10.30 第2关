#include<stdio.h>
	int main(void)
	{ int a,b,c,max,min,bigest,zuixiao; 
	  /*********Begin*********/
	 
      scanf("%d%d%d",&a,&b,&c);
	  max=a-b;
      if(max<0)
     {
      max=b;  //max=7
      min=a;  //min=5
      }
      else
     {
      max=a;
      min=b;
      }
      bigest=max-c;  //bigest=7-6
      if(bigest<0)
     { bigest=c;}
      else            //bigest=7
     { bigest=max;
      zuixiao=min-c;} //zuixiao=5-6
      if(zuixiao<0) 
      {zuixiao=min;  //zuixiao=5
      max=c;}
      else
     { zuixiao=c;
      max=min;}
      printf("max = %d\nmin = %d",bigest,zuixiao);
    
	  /*********End**********/ 
       return 0;
	}
