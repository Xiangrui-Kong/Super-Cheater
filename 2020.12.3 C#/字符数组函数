#include<stdio.h>
#include<string.h>

int main()
{
    char str1[2014] = { '\0' }, str2[2014] = { '\0' };

    scanf("%s", &str1);

    scanf("%s", &str2);

    if (strcmp(str1, str2))
    {
        if (strlen(str1) > strlen(str2))
        {
            strcat(str1, str2);
            printf("%s", str1);
        }
        else if (strlen(str1) < strlen(str2))
        {
            strcat(str2, str1);
            printf("%s", str2);
        }
        else
        {
            printf("%s%s",str1,str2);
        }
    }
    else if(strcmp(str1,str2)==0)
    {
        
        printf("%s",str1);
    }

    return 0;
}
